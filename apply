[32m+ def stock_dict(stock_symbol, stock_name):[m
[32m+     stock_name=[][m
[32m+     for dataset in stock_symbol:[m
[32m+         dataset.remove(stock_symbol)[m
[32m+         stock_name.append(dataset)[m
[32m+         stocks[stock_symbol] = stock_name[m
